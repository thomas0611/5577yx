@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/Scripts/Admin/ui/skins/Aqua/css/ligerui-all.css" rel="stylesheet" />
    <link href="~/Style/Admin/style.css" rel="stylesheet" />
    <link href="~/Style/Admin/pagination.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script src="~/Scripts/Admin/jquery.form.js"></script>
    <script src="~/Scripts/Admin/AdminFunction.js"></script>
    <script>
        $(document).ready(function () {
            InitPageContent("Lock");
        });
    </script>
</head>
<body class="mainbody">
    <div class="navigation">
        首页 &gt; 用户管理 &gt; 用户列表
    </div>
    <div class="tools_box">
        <div class="tools_bar">
            <div class="search_box">
                <form id="form1" onsubmit="return UpdateData('/GameUser/AddLock');">
                    添加Ip/Action锁定：<input id="LockInfo" name="LockInfo" />
                    <input type="submit" class="btnSubmit" value="添加锁定" />
                </form>
            </div>
        </div>
        <div class="select_box">
        </div>
    </div>
    <table width="100%" border="0" cellspacing="0" cellpadding="0" class="msgtable" style="text-align: center">
        <thead>
            <tr>
                <th width="5%">ID
                </th>
                <th>IP/Action
                </th>
                <th>添加时间</th>
                <th>附加信息</th>
                <th>操作者</th>
                <th>操作</th>
            </tr>
        </thead>
        <tbody id="Lock">
        </tbody>
    </table>
    <div class="page_box">
        <div id="PageContent" runat="server" class="flickr right">
        </div>
        <div class="left">
            显示<input name="txtPageNum" type="text" value="10" onchange="InitPageContent('Lock')" onkeyup="this.value=this.value.replace(/\D/g,'')"
                onafterpaste="this.value=this.value.replace(/\D/g,'')" id="txtPageNum" class="txtInput2 small2" />条/页
        </div>
    </div>
    <div class="line10">
    </div>
</body>
</html>

