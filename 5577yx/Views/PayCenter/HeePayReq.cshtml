@{
    Layout = null;
}

<!DOCTYPE html>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body onload="document.frmSubmit.submit();">
    <form id="frmSubmit" method="post" name="frmSubmit" action="https://pay.heepay.com/Payment/Index.aspx">
        <input type="hidden" name="version" value="@ViewData["version"]" />
        <input type="hidden" name="agent_id" value="@ViewData["agent_id"]" />
        <input type="hidden" name="agent_bill_id" value="@ViewData["agent_bill_id"]" />
        <input type="hidden" name="agent_bill_time" value="@ViewData["agent_bill_time"]" />
        <input type="hidden" name="pay_type" value="@ViewData["pay_type"]" />
        <input type="hidden" name="pay_code" value="@ViewData["pay_code"]" />
        <input type="hidden" name="pay_amt" value="@ViewData["pay_amt"]" />
        <input type="hidden" name="notify_url" value="@ViewData["notify_url"]" />
        <input type="hidden" name="return_url" value="@ViewData["return_url"]" />
        <input type="hidden" name="user_ip" value="@ViewData["user_ip"]" />
        <input type="hidden" name="goods_name" value="@ViewData["goods_name"]" />
        <input type="hidden" name="goods_num" value="@ViewData["goods_num"]" />
        <input type="hidden" name="goods_note" value="@ViewData["goods_note"]" />
        <input type="hidden" name="is_test" value="@ViewData["is_test"]" />
        <input type="hidden" name="remark" value="@ViewData["remark"]" />
        <input type="hidden" name="sign" value="@ViewData["sign"]" />
    </form>
</body>
</html>
