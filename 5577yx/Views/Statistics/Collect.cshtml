@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Collect</title>
    <link href="~/Scripts/Admin/ui/skins/Aqua/css/ligerui-all.css" rel="stylesheet" />
    <link href="~/Style/Admin/style.css" rel="stylesheet" />
    <link href="~/Style/Admin/pagination.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script src="~/Scripts/Admin/AdminFunction.js"></script>
    <script>
        $(document).ready(function () {
            GetAllGame();
            InitPageContent("Collect");
        });
    </script>
</head>
<body class="mainbody">
    <div class="navigation">
        首页 &gt; 统计管理 &gt; 汇总表 
    </div>
    <div class="tools_box">
        <div class="tools_bar">
            <div style="padding-top:8px;font-size:14px;" ><a href="/Statistics/GetSumMoney">充值排行统计</a></div>
            <div class="search_box">
            </div>
        </div>
        <div class="select_box">
            游戏：
            <select id="Selyx" class="select2" onchange="onyxchange('Collect')">
                <option value="0">所有游戏</option>
            </select>
            低于：<input id="MinMoney" type="number" class="txtInput" style="width: 50px;" min="0" value="500" onchange="InitPageContent('Collect')" onkeyup="this.value=this.value.replace(/\D/g,'')" /><span style="color: green">绿色显示</span>
            高于：<input id="MaxMoney" type="number" class="txtInput" style="width: 50px;" min="0" value="1000" onchange="InitPageContent('Collect')" onkeyup="this.value=this.value.replace(/\D/g,'')" /><span style="color: red">红色显示</span>
        </div>
    </div>
    <!--列表展示.开始-->
    <div class="photo_list3 clearfix">
        <ul id="Collect">
        </ul>
    </div>
    <!--列表展示.结束-->
    <div class="line15">
    </div>
    <div class="page_box">
        <div id="PageContent" runat="server" class="flickr right">
        </div>
        <div class="left">
            显示<input name="txtPageNum" type="text" value="9" onchange="InitPageContent('Collect')" onkeyup="this.value=this.value.replace(/\D/g,'')"
                onafterpaste="this.value=this.value.replace(/\D/g,'')" id="txtPageNum" class="txtInput2 small2" />条/页

        </div>
    </div>
    <div class="line10">
    </div>
</body>
</html>
