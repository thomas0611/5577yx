@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PromoAnalysis</title>
    <link href="~/Scripts/Admin/ui/skins/Aqua/css/ligerui-all.css" rel="stylesheet" />
    <link href="~/Style/Admin/style.css" rel="stylesheet" />
    <link href="~/Style/Admin/pagination.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script src="~/Scripts/Admin/AdminFunction.js"></script>
    <script>
        $(document).ready(function () {
            InitPageContent("PromoAnalysis");
        });
    </script>
</head>
<body class="mainbody">
    <div class="navigation">
        首页 &gt; 统计管理 &gt; 推广分析 
    </div>
    <div class="tools_box">
        <div class="tools_bar"><div style="text-align:center;float:left;margin:10px 0 0 10px;"><a href="/Statistics/PromoByBengBeng/" target="_self">蹦蹦网推广统计</a></div> 
            <div class="search_box">
                用户名：
                <input id="UserName" class="txtInput" style="width: 120px;" onchange="InitPageContent('PromoAnalysis')">
                推广链接参数：
                <input id="LinkName" class="txtInput" style="width: 120px;" onchange="InitPageContent('PromoAnalysis')">
                <input type="button" id="btnSearch" value="搜 索" class="btnSearch" onclick="InitPageContent('PromoAnalysis');" /> 
            </div>
        </div>
        <div class="select_box">
        </div>
    </div>
    <!--列表展示.开始-->
    <table width="100%" border="0" cellspacing="0" cellpadding="0" class="msgtable" style="text-align: center">
        <thead>
            <tr>
                <th width="5%">ID
                </th>
                <th>用户名
                </th>
                <th>推广员角色</th>
                <th>推广用户</th>
                <th>推广总额    
                </th>
                <th width="50">操作
                </th>
            </tr>
        </thead>
        <tbody id="PromoAnalysis">
        </tbody>
    </table>
    <!--列表展示.结束-->
    <div class="line15">
    </div>
    <div class="page_box">
        <div id="PageContent" runat="server" class="flickr right">
        </div>
        <div class="left">
            显示<input name="txtPageNum" type="text" value="10" onchange="InitPageContent('PromoAnalysis')" onkeyup="this.value=this.value.replace(/\D/g,'')"
                onafterpaste="this.value=this.value.replace(/\D/g,'')" id="txtPageNum" class="txtInput2 small2" />条/页

        </div>
    </div>
    <div class="line10">
    </div>
</body>
</html>
